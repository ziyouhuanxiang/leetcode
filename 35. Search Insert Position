int searchInsert(int* nums, int numsSize, int target) {
    int i = 0;
    
    int left = 0,right=numsSize-1;
    int index  = (left+right)/2;
    while(left <= right){   
        if(nums[index]== target){
            return index;
        }
        else if(nums[index]< target){
            left = index+1;
        }else{
            right = index-1;
        }
        index  = (left+right)/2;
    }
    return left;
}
